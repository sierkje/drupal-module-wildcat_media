{
  "name": "drupal/wildcat_media",
  "description": "Improved media handling for Wildcat-flavored sites.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Sierk Beij",
      "email": "sierk@cococollective.org"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-8.x-0.x": "0.0.x-dev"
    }
  },
  "replace": {
    "drupal/wildcat_media_document": "self.version",
    "drupal/wildcat_media_gallery": "self.version",
    "drupal/wildcat_media_image": "self.version",
    "drupal/wildcat_media_instagram": "self.version",
    "drupal/wildcat_media_twitter": "self.version",
    "drupal/wildcat_media_video": "self.version"
  },
  "require": {
    "drupal/dropzonejs": "^1.0",
    "drupal/entity_browser": "^1.0",
    "drupal/entity_embed": "^1.0",
    "drupal/image_widget_crop": "^1.4",
    "drupal/inline_entity_form": "^1.0",
    "drupal/masonry_views": "^1.0",
    "drupal/media_entity": "^1.0",
    "drupal/media_entity_document": "^1.0",
    "drupal/media_entity_image": "^1.0",
    "drupal/media_entity_instagram": "^1.0",
    "drupal/media_entity_slideshow": "^1.2",
    "drupal/media_entity_twitter": "^1.3",
    "drupal/video_embed_field": "^1.0",
    "drupal/slick_media": "^1.0",
    "drupal/views_infinite_scroll": "^1.1"
  }
}
